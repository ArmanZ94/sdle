"Refract"
{
	"$model" "0"
	
	"$basetexture" "dlenstexture/dlensdefault"
	
	"$translucent" 1
	"$refractamount" ".01"

	"$levelr" "0.5" 
	"$levelg" "0.5" 
	"$levelb" "0.5" 
	"$ignorez" "1"
	
	"$additive" "1"
	
	"$dudvmap" "dlenstexture/dlensdefault"
	"$normalmap" "dlenstexture/dlensdefault"
	"$refracttinttexture" "dlenstexture/dlensdefault"
	
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$refracttinttexture"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

	}

}
